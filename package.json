{
  "name": "vite_vitest_vue_vuetify",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/main.js",
  "volta": {
    "node": "23.11.0",
    "npm": "11.2.0"
  },
  "scripts": {
    "build": "npm run type-check && vite build && cp ./src/main/main.ts ./dist/main.js && cp -r ./src/@assets ./dist/",
    "start": "npm-run-all2 --parallel start:vite start:electron",
    "start:electron": "set NODE_ENV=development && electron .",
    "start:vite": "vite",
    "test": "vitest",
    "test:e2e": "playwright test --project=chromium",
    "type-check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@mdi/font": "^7.4.47",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.1.4",
    "happy-dom": "^17.4.4",
    "npm-run-all2": "^7.0.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vitest": "^3.1.1",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8",
    "vuetify": "^3.8.0"
  }
}
