{
  "name": "vite_vitest_vue_vuetify",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/main.js",
  "volta": {
    "node": "24.0.2",
    "npm": "11.4.0"
  },
  "scripts": {
    "build": "npm run type-check && vite build && cp ./src/main/main.ts ./dist/main.js && cp -r ./src/@assets ./dist/",
    "start": "npm-run-all2 --parallel start:vite --race start:electron",
    "start:electron": "set NODE_ENV=development && electron .",
    "start:vite": "vite",
    "test": "vitest",
    "test:e2e": "playwright test --project=chromium",
    "type-check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@mdi/font": "^7.4.47",
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^36.4.0",
    "happy-dom": "^18.0.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10",
    "vuetify": "^3.8.9"
  }
}
